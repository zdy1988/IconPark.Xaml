            <PathGeometry FillRule="Nonzero" Figures="M30,30C30,33.3137 27.3137,36 24,36 20.6863,36 18,33.3137 18,30 18,26.6863 20.6863,22.5 24,22.5 27.3137,22.5 30,26.6863 30,30z" />
          </GeometryDrawing.Geometry>
        </GeometryDrawing>
      </DrawingGroup.Children>
      <DrawingGroup.ClipGeometry>
        <RectangleGeometry Rect="0,0,48,48" />
      </DrawingGroup.ClipGeometry>
    </DrawingGroup>
  </DrawingImage.Drawing>
</DrawingImage></Image.Source>
</Image>
</Border>
</ControlTemplate>
</ResourceDictionary>
