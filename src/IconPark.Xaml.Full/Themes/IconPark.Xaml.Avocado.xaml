          <GeometryDrawing.Geometry>
            <PathGeometry FillRule="Nonzero" Figures="M33,19C31,14 33,4 24,4 15,4 17,14 15,18 13,22 10,25 10,31 10,40 17.5,44 24,44 30.5,44 38,40 38,31 38,26 35,24 33,19z" />
