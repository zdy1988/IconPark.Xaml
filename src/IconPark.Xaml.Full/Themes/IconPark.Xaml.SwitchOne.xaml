          <GeometryDrawing.Geometry>
            <RectangleGeometry RadiusX="5.2594" RadiusY="5.2594" Rect="36.0063,19.3335,10.5189,24.0125" Transform="6.123233995736766E-17,1,-1,6.123233995736766E-17,55.3398,-16.672800000000002" />
          </GeometryDrawing.Geometry>
        </GeometryDrawing>
        <GeometryDrawing Brush="{x:Null}">
